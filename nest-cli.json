{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/hari-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/hari-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/hari-app",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "comment-service": {
      "type": "application",
      "root": "apps/comment-service",
      "entryFile": "main",
      "sourceRoot": "apps/comment-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/comment-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "media-service": {
      "type": "application",
      "root": "apps/media-service",
      "entryFile": "main",
      "sourceRoot": "apps/media-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/media-service/tsconfig.app.json"
      }
    },
    "news-service": {
      "type": "application",
      "root": "apps/news-service",
      "entryFile": "main",
      "sourceRoot": "apps/news-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/news-service/tsconfig.app.json"
      }
    },
    "notification-service": {
      "type": "application",
      "root": "apps/notification-service",
      "entryFile": "main",
      "sourceRoot": "apps/notification-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-service/tsconfig.app.json"
      }
    },
    "post-service": {
      "type": "application",
      "root": "apps/post-service",
      "entryFile": "main",
      "sourceRoot": "apps/post-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/post-service/tsconfig.app.json"
      }
    },
    "proto": {
      "type": "library",
      "root": "libs/proto",
      "entryFile": "index",
      "sourceRoot": "libs/proto/src",
      "compilerOptions": {
        "tsConfigPath": "libs/proto/tsconfig.lib.json"
      }
    },
    "reaction-service": {
      "type": "application",
      "root": "apps/reaction-service",
      "entryFile": "main",
      "sourceRoot": "apps/reaction-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/reaction-service/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    }
  }
}